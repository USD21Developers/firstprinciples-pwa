<?xml version="1.0" encoding="UTF-8"?>

<scripture key="ephesians-5-18-19" lang="en" version="niv" edition="1984">
	<passage title="Ephesians 5:18-19" book="Ephesians" book-en="Ephesians">
		<chapter number="5" number-en="5">
			<verse number="18" number-en="18">
				Do not get drunk on wine, which leads to debauchery. Instead, be filled with the Spirit.
			</verse>
			<verse number="19" number-en="19">
				Speak to one another with psalms, hymns and spiritual songs. Sing and make music in your heart to the Lord,
			</verse>
		</chapter>
	</passage>
</scripture>

