<?xml version="1.0" encoding="UTF-8"?>

<scripture key="matthew-7-13-14" lang="en" version="niv" edition="1984">
	<passage title="Matthew 7:7" book="Matthew" book-en="Matthew">
		<chapter number="7" number-en="7">
			<verse number="13" number-en="13" jesuswords="true">
				<p class="scripture-heading">
					The Narrow and Wide Gates
				</p>
				<span class="jesuswords">Enter through the narrow gate. For wide is the gate and broad is the road that leads to destruction, and many enter through it.</span>
			</verse>
			<verse number="14" number-en="14" jesuswords="true">
				<span class="jesuswords">But small is the gate and narrow the road that leads to life, and only a few find it.</span>
			</verse>
		</chapter>
	</passage>
</scripture>

